{
  "name": "penguin-games",
  "version": "official-1.0.0",
  "description": "Create and play games by the community, for the community.",
  "scripts": {
    "start": "concurrently \"npm start --prefix server\" \"npm start --prefix client\"",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "npm run dev --prefix server",
    "dev-client": "npx serve -l 0100 ./client --cors"
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  },
  "keywords": [
    "games",
    "social",
    "gaming",
    "editor",
    "multiplayer",
    "open-source"
  ],
  "author": "Riley",
  "license": "GPL-3.0-only"
}
